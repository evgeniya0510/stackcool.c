#include <stdio.h>

#include <malloc.h>


int main()
{
	int *matr, i, n;
	printf("Vvedite razmer massiva dlya sortirovki bubble: ");
    scanf_s("%i", &n);
    matr = (int*) malloc(n*sizeof(int));
	for(i = 0; i < n; i++)
    {
		printf("mas_1[%d] = ", i);
		scanf_s("%i", &matr[i]);
    }
	for(i = 0; i < n; i++)
	{
		matr[i] = matr[i] * matr[i];
	}
	printf("Rezult: ");
	for (i = 0; i < n; i++)
	{
		printf("%i ", matr[i]);
	}
}
